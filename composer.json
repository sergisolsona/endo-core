{
    "name": "endo/endo-core",
    "description": "Endo's framework core",
    "version": "0.1",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7",
        "laravel/framework": "^5.6",
        "astrotomic/laravel-translatable": "^11.3"
    },
    "autoload": {
        "psr-4": {
            "Endo\\EndoCore\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Endo\\EndoCore\\EndoCoreServiceProvider"
            ]
        }
    }
}
